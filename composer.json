{
    "name": "miguilim/laravel-singlestore-backup",
    "description": "A package that makes it easy to make backups of your SingleStore database.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Miguilim\\LaravelSinglestoreBackup\\": "src/"
        }
    },
    "require": {
        "illuminate/config": "^9.0|^10.2",
        "illuminate/console": "^9.0|^10.2",
        "illuminate/database": "^9.0|^10.2",
        "illuminate/support": "^9.0|^10.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miguilim\\LaravelSinglestoreBackup\\LaravelSinglestoreBackupServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
